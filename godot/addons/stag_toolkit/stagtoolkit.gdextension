[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.2
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://addons/stag_toolkit/bin/debug/libStagToolkit.so"
linux.release.x86_64 =   "res://addons/stag_toolkit/bin/release/libStagToolkit.so"
windows.debug.x86_64 =   "res://addons/stag_toolkit/bin/release/StagToolkit.dll"
windows.release.x86_64 = "res://addons/stag_toolkit/bin/release/StagToolkit.dll"
macos.debug =            "res://addons/stag_toolkit/bin/debug/libStagToolkit.dylib"
macos.release =          "res://addons/stag_toolkit/bin/release/libStagToolkit.dylib"
macos.debug.arm64 =      "res://addons/stag_toolkit/bin/debug/libStagToolkit.dylib"
macos.release.arm64 =    "res://addons/stag_toolkit/bin/release/libStagToolkit.dylib"

[icons]
IslandBuilder = "res://addons/stag_toolkit/icons/icon_islandbuilder.svg"
IslandBuilderShape = "res://addons/stag_toolkit/icons/icon_islandbuildershape.svg"
NavIslandProperties = "res://addons/stag_toolkit/icons/icon_islandbuilder.svg"
