[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.4
reloadable = false

[libraries]
linux.debug.x86_64 =     "res://addons/stag_toolkit/bin/release/libstag_toolkit.so"
linux.release.x86_64 =   "res://addons/stag_toolkit/bin/release/libstag_toolkit.so"

windows.debug.x86_64 =   "res://addons/stag_toolkit/bin/release/stag_toolkit.dll"
